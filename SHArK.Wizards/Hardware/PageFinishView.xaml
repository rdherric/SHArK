<SF:WizardPage x:Class="SHArK.Wizards.Hardware.PageFinishView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:RW="http://schemas.rdh2.com/windows/markup"
    xmlns:RES="http://schemas.rdh2.com/shark/common"
    xmlns:SF="http://schemas.syncfusion.com/wpf"
    Title="{x:Static RES:Strings.HardwareWizardFinishTitle}" Description="{x:Static RES:Strings.HardwareWizardFinishDesc}"
    PageType="Interior" BannerImage="{RW:ImageResource AssemblyName={x:Static RES:Strings.SHArKCommonName}, ImagePath={x:Static RES:Strings.HomeButtonImageSource}}">
    <TextBlock Text="{x:Static RES:Strings.HardwareWizardFinishText}" TextWrapping="Wrap" />
</SF:WizardPage>
